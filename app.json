{
  "expo": {
    "name": "AlatPres Mobile",
    "slug": "Alatpres-Mobile",
    "description":"Reliable Public Response Partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-remake.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/full_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wakieye.alatpres"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-remake.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.wakieye.alatpres"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
   
  }
}
